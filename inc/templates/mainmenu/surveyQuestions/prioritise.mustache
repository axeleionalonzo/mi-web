<div class="row fit-bottom" question-id="{{id}}">
	<div class="col s12">
	    <p class="flow-text">{{question}}</p>
	</div>
	<div class="container">
		<div class="row">
			<div class="col s12">
				<ul id="items" class="collection draggable collection-border">
					{{#answers}}
				    <li id="{{id}}" class="collection-item">{{answer}}</li>
					{{/answers}}
				</ul>
			</div>
		</div>
	</div>
</div>