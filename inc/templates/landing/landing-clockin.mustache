<div class="input-field col s12">
	<div class="clock-holder">
		<p id="clockintime" class="center flow-text clockintime"></p>
		<p id="clockindate" class="center clockindate"></p>
	</div>
	<a id="clockin_confirm" class="waves-effect waves-light btn col s12 landing-next btn-large"><i class="material-icons right yellow-text">access_time</i>Clock in</a>
</div>


<!-- Modals -->
<div id="clockin_modal" class="modal bottom-sheet">
	<div class="modal-content">
		<p id="clockin_message" class="center flow-text"></p>
	</div>
	<div class="modal-footer">
		<a id="clockin_proceed" class="modal-action modal-close waves-effect waves-green btn-flat pull-left teal-text">Sure</a>
		<a class="modal-action modal-close waves-effect waves-red btn-flat grey-text text-darken-1">Later</a>
	</div>
</div>

<div id="logout_modal" class="modal bottom-sheet">
	<div class="modal-content">
		<p>You will be logged out from Market Intel. Do you want to proceed?</p>
	</div>
	<div class="modal-footer">
		<a id="logout_proceed" class="modal-action modal-close waves-effect waves-green btn-flat pull-left teal-text">Sure</a>
		<a class="modal-action modal-close waves-effect waves-red btn-flat">I changed my mind</a>
	</div>
</div>